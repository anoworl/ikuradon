{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-alias",
          [
            {
              "src": "./app",
              "expose": "app"
            },
            {
              "src": "./assets",
              "expose": "assets"
            }
          ]
        ]
      ]
    }
  }
}